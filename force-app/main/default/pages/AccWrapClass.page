<apex:page controller="AccWrapClass">
<apex:form >
<apex:pageBlock >
<apex:pageBlockButtons >
<apex:commandButton value="Add Record" action="{!selectRecord}"/>
</apex:pageBlockButtons>
<apex:pageBlockTable value="{!wc}" var="w">
<apex:column >
<apex:inputCheckbox value="{!w.b}"/> 
</apex:column>
<apex:column value="{!w.a.name}"/>
<apex:column value="{!w.a.industry}"/>
<apex:column value="{!w.a.type}"/>
<apex:column value="{!w.a.phone}"/>
</apex:pageBlockTable>

<apex:pageBlockTable value="{!accList}" var="accL">
<apex:column value="{!accL.name}"/>
<apex:column value="{!accL.industry}"/>
<apex:column value="{!accL.type}"/>
<apex:column value="{!accL.phone}"/>
</apex:pageBlockTable>
</apex:pageBlock>
</apex:form>
</apex:page>